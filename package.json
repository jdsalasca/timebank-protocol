{
  "name": "timebank-protocol",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "apps/web-react",
    "packages/*"
  ],
  "scripts": {
    "balances": "node src/compute-balances.mjs examples/ledger.json",
    "dev:web": "npm --workspace apps/web-react run dev",
    "build:web": "npm --workspace apps/web-react run build",
    "preview:web": "npm --workspace apps/web-react run preview",
    "agent:gate": "node scripts/agent-gate.mjs",
    "quality:quick": "npm run balances && npm run build:web && npm run agent:gate",
    "quality:release": "node scripts/release-quality-gate.mjs"
  },
  "releaseQuality": {
    "minimumCoverage": 0
  }
}
